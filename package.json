{
  "name": "cline-code-nexus",
  "private": true,
  "devDependencies": {
    "@types/bun": "latest",
    "copyfiles": "^2.4.1"
  },
  "peerDependencies": {
    "typescript": "^5.8.3"
  },
  "scripts": {
    "dev:frontend": "set NODE_ENV=development&& bun run --hot src/index.tsx",
    "dev:backend": "set NODE_ENV=development&& bun run server/index.ts",
    "dev": "concurrently \"bun run dev:frontend\" \"bun run dev:backend\"",
    "build": "bun build src/index.tsx --outdir ./build && copyfiles -u 1 \"public/**/*\" build"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@types/bcrypt": "^5.0.2",
    "@types/marked": "^6.0.0",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "bcrypt": "^5.1.1",
    "buffer": "^6.0.3",
    "bun": "^1.3.6",
    "concurrently": "^9.2.1",
    "hono": "^4.11.4",
    "marked": "^16.4.2",
    "music-metadata-browser": "^2.5.11",
    "pino": "^9.14.0",
    "pino-pretty": "^13.1.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-syntax-highlighter": "^15.6.6",
    "sharp": "^0.34.5"
  }
}
